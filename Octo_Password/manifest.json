{
    "manifest_version": 2,
    "name": "Octo Password",
    "version": "1.0",
    "description": "Une extension Chrome pour générer des mots de passe",
    "browser_action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
      }
    },
    "permissions": ["clipboardWrite"],
    "icons": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  }
  